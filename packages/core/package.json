{
  "name": "@supertools-ai/core",
  "version": "0.1.1",
  "description": "Programmatic tool calling for LLMs - let AI write code that orchestrates your tools",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build": "bun run clean && bun build src/index.ts --outdir dist --target node --format esm && tsc --emitDeclarationOnly",
    "dev": "bun --watch src/index.ts",
    "test": "bun test",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "cp ../../README.md . && bun run build && bun run test"
  },
  "dependencies": {
    "e2b": "^2.9.0",
    "zod": "^4.2.1"
  },
  "peerDependencies": {
    "@anthropic-ai/sdk": ">=0.30.0"
  },
  "peerDependenciesMeta": {
    "@anthropic-ai/sdk": {
      "optional": true
    }
  },
  "devDependencies": {
    "@anthropic-ai/sdk": "^0.30.0",
    "@types/bun": "latest",
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0"
  },
  "keywords": [
    "supertools",
    "llm",
    "tool-calling",
    "programmatic",
    "code-execution",
    "e2b",
    "sandbox",
    "anthropic",
    "openai",
    "ai",
    "agents",
    "claude"
  ],
  "author": "Filip Brebera",
  "license": "MIT",
  "homepage": "https://github.com/bxxf/supertools#readme",
  "bugs": {
    "url": "https://github.com/bxxf/supertools/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bxxf/supertools.git",
    "directory": "packages/core"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false
}
